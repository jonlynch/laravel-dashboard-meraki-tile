{
    "name": "jonlynch/laravel-dashboard-meraki-tile",
    "description": "A package to display Meraki device and client status",
    "keywords": [
        "laravel-dashboard",
        "jonlynch"
    ],
    "homepage": "https://github.com/jonlynch/laravel-dashboard-meraki-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Lynch",
            "email": "jon.lynch@remove.ymail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "spatie/laravel-dashboard": ">=1.0 <3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "JonLynch\\MerakiTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JonLynch\\MerakiTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JonLynch\\MerakiTile\\MerakiTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}